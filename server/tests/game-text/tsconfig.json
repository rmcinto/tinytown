{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "baseUrl": ".",
        "paths": {
            "entities/*": [
                "../entities/*"
            ]
        },
        "outDir": "./dist",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "strict": true,
        "resolveJsonModule": true,
        "allowSyntheticDefaultImports": true
    }
}